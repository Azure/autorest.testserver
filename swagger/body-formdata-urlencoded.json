{
  "swagger": "2.0",
  "info": {
    "title": "Body Forms Data URLEncoded",
    "description": "Test Infrastructure for AutoRest Swagger BAT",
    "version": "1.0.0"
  },
  "paths": {
    "/formsdataurlencoded/pet/add": {
      "post": {
        "tags": [
          "formsdataurlencoded"
        ],
        "summary": "Updates a pet in the store with form data",
        "description": "",
        "operationId": "formdataurlencoded_updatePetWithForm",
        "consumes": [
          "application/x-www-form-urlencoded"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "petId",
            "in": "path",
            "description": "ID of pet that needs to be updated",
            "required": true,
            "type": "string"
          },
          {
            "$ref": "#/parameters/Pet_type"
          },
          {
            "$ref": "#/parameters/Pet_food"
          },
          {
            "name": "name",
            "in": "formData",
            "description": "Updated name of the pet",
            "required": false,
            "type": "string"
          },
          {
            "name": "status",
            "in": "formData",
            "description": "Updated status of the pet",
            "required": false,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "Pet updated."
          },
          "405": {
            "description": "Invalid input"
          }
        }
      }
    }
  },
  "parameters": {
    "Pet_type": {
        "name": "pet_type",
        "description": "Can take a value of dog, or cat, or fish",
        "type": "string",
        "in": "formData",
        "required": true,
        "enum": [
            "dog",
            "cat",
            "fish"
        ],
        "x-ms-parameter-location": "method"
    },
    "Pet_food": {
      "name": "pet_food",
      "description": "Can take a value of meat, or fish, or plant",
      "type": "string",
      "in": "formData",
      "required": true,
      "enum": [
          "meat",
          "fish",
          "plant"
      ],
      "x-ms-parameter-location": "method"
  }
  }
}